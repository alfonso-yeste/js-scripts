var paquetesIps = [];
var paquete = "";

for(let x = 0; x < ips.length; x++){

    if ((paquete + '|' + ips[x]).length > 256 ) {
        paquetesIps.push(paquete);
        paquete=ips[x];
    } else {

        if(paquete!=""){
            paquete+='|';
        }
        paquete+=ips[x];
    }
}

console.log(paquetesIps);
